{
	"manifest_version": 2,

	"name": "FIFO queue tabs manager",
	"description": "Saving tabs adresses URLs (URIs) to file and making lists of it, making load them one by one",
	"version": "0.5",
// not working with manifest v3
	"background": {
		"scripts": ["scripts/background.js"]
	},

	"permissions": [
		"tabs",
		"storage"
	],
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_title": "FIFO queue tabs manager",
		"default_popup": "pages/popup.html"
	}
}